{
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "java.configuration.runtimes": [],
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.project.excludePaths": [
    "**/node_modules/**",
    "**/android/**/node_modules/**",
    "**/android/build/**"
  ],
  "java.project.importHint.enabled": false,
  "java.project.importOnFirstTimeStartup": "disabled",
  "java.project.sourcePaths": []
}
